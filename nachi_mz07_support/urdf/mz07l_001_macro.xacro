<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  
  <xacro:macro name="nachi_mz07l_001" params="prefix">
    <!-- LINKS -->
    <!-- base link -->
    <link name="${prefix}base_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/base_link.stl" />
        </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/base_link.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 1 (A1) -->
    <link name="${prefix}link_1">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_1.stl" />
        </geometry>
      <material name="white">
        <color rgba="1.0 1.0 1.0 1"/>
      </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_1.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 2 -->
    <link name="${prefix}link_2">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_2.stl"/>
        </geometry>
        <material name="white"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_2.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 3 -->
    <link name="${prefix}link_3">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_3.stl" />
        </geometry>
        <material name="white"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_3.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 4 -->
    <link name="${prefix}link_4">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_4.stl" />
        </geometry>
        <material name="white"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_4.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 5 -->
    <link name="${prefix}link_5">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_5.stl" />
        </geometry>
        <material name="white"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_5.stl" />
        </geometry>
      </collision>
    </link>
    <!-- link 6 -->
    <link name="${prefix}link_6">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/visual/link_6.stl" />
        </geometry>
        <material name="white"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://nachi_mz07_support/meshes/mz07l_001/collision/link_6.stl" />
        </geometry>
      </collision>
    </link>
    <!-- tool 0 -->
    <!-- This frame corresponds to the $FLANGE coordinate system in  KRC controllers. -->
    <link name="${prefix}tool0"/>
    <!-- END LINKS -->

    <!-- JOINTS -->
    <!-- joint 1 (A1) -->
    <joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.345" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="${radians(-170)}" upper="${radians(170)}" velocity="${radians(450)}"/>
    </joint>
    <!-- joint 2 (A2) -->
    <joint name="${prefix}joint_2" type="revolute">
      <origin xyz="0.050 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="${radians(-135)}" upper="${radians(80)}" velocity="${radians(280)}"/>
    </joint>
    <!-- joint 3 (A3) -->
    <joint name="${prefix}joint_3" type="revolute">
      <origin xyz="0.420 0 0" rpy="0 ${radians(+90)} 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="${radians(-139)}" upper="${radians(270)}" velocity="${radians(360)}"/>
    </joint>
    <!-- joint 4 (A4) -->
    <joint name="${prefix}joint_4" type="revolute">
      <origin xyz="0.440 0 0.045" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="${radians(-190)}" upper="${radians(190)}" velocity="${radians(550)}"/>
    </joint>
    <!-- joint 5 (A5) -->
    <joint name="${prefix}joint_5" type="revolute">
      <origin xyz="0.0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="${radians(-120)}" upper="${radians(120)}" velocity="${radians(550)}"/>
    </joint>
    <!-- joint 6 (A6) -->
    <joint name="${prefix}joint_6" type="revolute">
      <origin xyz="0.073 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="${radians(-360)}" upper="${radians(360)}" velocity="${radians(1000)}"/>
    </joint>
    <!-- tool frame - fixed frame -->
    <joint name="${prefix}joint_6-tool0" type="fixed">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}tool0"/>
      <origin xyz="0.0 0 0" rpy="0 ${radians(90)} 0"/>
    </joint>
    <!-- END JOINTS -->

    <!-- ROS base_link to  $ROBROOT coordinate system transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

  </xacro:macro>
</robot>
